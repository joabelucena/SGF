<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified"
	elementFormDefault="qualified" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://localhost/eventsService">
	
	<xs:element name="EventRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element type="xs:string" name="equipment_id" />
				<xs:element type="xs:string" name="equipment_site" />
				<xs:element type="xs:string" name="equipment_model" />
				<xs:element type="xs:string" name="equipment_state" />
				<xs:element type="xs:string" name="event_code" />
				<xs:element type="xs:date" name="date" />
				<xs:element type="xs:time" name="time" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
</xs:schema>